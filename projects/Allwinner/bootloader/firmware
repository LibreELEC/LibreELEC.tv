[ -n "$ATF_PLATFORM" ] && cp -av $(get_install_dir atf)/usr/share/bootloader/bl31.bin .

CRUST_CONFIG=$($ROOT/$SCRIPTS/build_matrix $PROJECT $DEVICE $UBOOT_SYSTEM crust_config)
[ -n "$CRUST_CONFIG" ] && cp -av $(get_install_dir crust)/usr/share/bootloader/scp.bin .

# Suppress errors from build_matrix so u-boot properly builds with and without crust
true
